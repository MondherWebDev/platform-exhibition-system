{
  "name": "event-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "clean-data": "npx tsx src/scripts/resetData.ts",
    "delete-old-event": "npx tsx src/scripts/resetData.ts --delete-old-event",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --config jest.config.js",
    "test:integration": "node -e \"console.log('Integration tests would run here')\"",
    "test:framework": "node -e \"const { eventPlatformTester } = require('./src/utils/testingFramework.ts'); eventPlatformTester.runCompleteTestSuite().then(results => console.log('Tests completed:', results.length, 'suites'))\"",
    "test:ui": "next dev --port 3001",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "backup:daily": "npx tsx src/scripts/automatedBackup.ts daily",
    "backup:full": "npx tsx src/scripts/automatedBackup.ts full",
    "backup:restore": "npx tsx src/scripts/automatedBackup.ts restore",
    "backup:verify": "npx tsx src/scripts/automatedBackup.ts verify",
    "optimize:performance": "npx tsx src/scripts/performanceOptimizer.ts full",
    "optimize:bundle": "npx tsx src/scripts/performanceOptimizer.ts bundle",
    "optimize:database": "npx tsx src/scripts/performanceOptimizer.ts database",
    "optimize:cache": "npx tsx src/scripts/performanceOptimizer.ts cache",
    "optimize:react": "npx tsx src/scripts/performanceOptimizer.ts react",
    "optimize:network": "npx tsx src/scripts/performanceOptimizer.ts network",
    "optimize:config": "npx tsx src/scripts/performanceOptimizer.ts config",
    "optimize:report": "npx tsx src/scripts/performanceOptimizer.ts report",
    "deploy:optimize": "npx tsx src/scripts/deploymentOptimizer.ts optimize",
    "deploy:checklist": "npx tsx src/scripts/deploymentOptimizer.ts checklist",
    "deploy:monitoring": "npx tsx src/scripts/deploymentOptimizer.ts monitoring",
    "test:complete": "npx tsx src/scripts/testCompletePlatform.ts complete",
    "test:database": "npx tsx src/scripts/testCompletePlatform.ts database",
    "test:redis": "npx tsx src/scripts/testCompletePlatform.ts redis",
    "test:users": "npx tsx src/scripts/testCompletePlatform.ts users",
    "test:badges": "npx tsx src/scripts/testCompletePlatform.ts badges",
    "test:matchmaking": "npx tsx src/scripts/testCompletePlatform.ts matchmaking",
    "test:performance": "npx tsx src/scripts/testCompletePlatform.ts performance"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20251011.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-fontawesome": "^3.0.2",
    "@sentry/nextjs": "^10.19.0",
    "@sentry/react": "^10.19.0",
    "@sentry/tracing": "^7.120.4",
    "@types/gsap": "^1.20.2",
    "@types/jspdf": "^1.3.3",
    "@types/next": "^8.0.7",
    "@types/nodemailer": "^7.0.2",
    "@types/qrcode": "^1.5.5",
    "@upstash/redis": "^1.35.5",
    "chart.js": "^4.5.0",
    "cloudflare": "^5.2.0",
    "cloudinary": "^2.7.0",
    "firebase": "^12.3.0",
    "framer-motion": "^12.23.16",
    "gsap": "^3.13.0",
    "html2canvas": "^1.4.1",
    "html5-qrcode": "^2.3.8",
    "jspdf": "^3.0.3",
    "jsqr": "^1.4.0",
    "next": "^15.5.3",
    "nodemailer": "^7.0.9",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/firebase": "^2.4.32",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/qrcode.react": "^1.0.5",
    "@types/react": "^19.1.13",
    "@types/react-chartjs-2": "^2.0.2",
    "@types/react-dom": "^19.1.9",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.4",
    "typescript": "^5"
  }
}
